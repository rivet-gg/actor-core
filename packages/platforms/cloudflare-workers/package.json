{
  "name": "@rivetkit/cloudflare-workers",
  "version": "0.9.0",
  "keywords": [
    "rivetkit",
    "cloudflare",
    "actors",
    "edge",
    "platform",
    "serverless"
  ],
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    },
    "./tsconfig": "./dist/tsconfig.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/mod.ts",
    "check-types": "tsc --noEmit",
    "test": "vitest run tests"
  },
  "peerDependencies": {
    "@rivetkit/core": "*",
    "hono": "4.8.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250129.0",
    "@types/invariant": "^2",
    "@types/node": "^24.0.3",
    "@rivetkit/core": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2",
    "vitest": "^3.1.1",
    "wrangler": "^4.22.0"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "zod": "^3.25.67"
  },
  "stableVersion": "0.8.0"
}
